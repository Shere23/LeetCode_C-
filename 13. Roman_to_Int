// Given a roman numeral, convert it to an integer.

int romanToInt(char * s){

    int multiplier = 1,res=0;
    while(*s != '\0'){
        int i = 0;
        printf("Itearation %d - Sum is %d , *s is %c \n",i+=1,res,*s);
        if(*s == 'I'){
            if(*(s+1) == 'V'){
                 res += 4; *s++; 
            }
            else if(*(s+1) == 'X'){
                res += 9; *s++;
            }
            else
                res += 1;
        }
        else if(*s == 'V')
           res += 5;
        else if(*s == 'X'){

                if(*(s+1) == 'L')
                {
                 res += 40; *s++; 
                }
                else if(*(s+1) == 'C')
                {
                 res += 90; *s++; 
                }
                else
                 res += 10;
        }
        else if(*s == 'L')
              res += 50;
        else if(*s == 'C'){
                if(*(s+1) == 'D'){
                 res += 400; *s++;
                } 
                else if(*(s+1) == 'M')
                 {
                    res += 900; *s++; 
                 }
                else
               res += 100;
        }
        else if(*s == 'D')
                res += 500;
        else if(*s == 'M')
                 res += 1000;


        *s++;
    }

    return res;

}
